<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:uc="http://www.quickzip.org/UserControls"
    >

    <uc:VirutalWrapPanelView x:Key="IconView"  ItemHeight="60" ItemWidth="50" HorizontalContentAlignment="Left" >
        <uc:VirutalWrapPanelView.ItemTemplate>
            <DataTemplate>
                <StackPanel Orientation="Vertical">
                    <Image x:Name="img" Source="Generic_Document.png" Width="32" HorizontalAlignment="Center" />
                    <TextBlock Text="{Binding}" Margin="5,0"  HorizontalAlignment="Center" />

                </StackPanel>
            </DataTemplate>
        </uc:VirutalWrapPanelView.ItemTemplate>
    </uc:VirutalWrapPanelView>
   
</ResourceDictionary>
